// Проверяем, новый ли заказ, если у шоппинг карта нет ордер айди, то присваиваем его айдишник корзине в форме нового заказа
order_id = <%= @shopping_cart.try(:order_id) %>
// без этой строчки if считывается некорректно
console.log('order_id ' + order_id)
if(!order_id){
  $('#shopping_cart_cell_').attr('id', 'shopping_cart_cell_<%= @shopping_cart.id %>')
}
$('#shopping_cart_cell_<%= @shopping_cart.id %>').hide()
$('#shopping_cart_cell_<%= @shopping_cart.id %>').html('<%= j render 'shopping_carts/shopping_cart', shopping_cart: @shopping_cart %>')
$('#shopping_cart_cell_<%= @shopping_cart.id %>').fadeIn()